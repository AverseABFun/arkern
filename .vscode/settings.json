{
    "rust-analyzer.linkedProjects": [
        "loader/x86/bios/stage2/Cargo.toml",
        "kernel/x86/multiboot/Cargo.toml"
    ],
    "rust-analyzer.cargo.allTargets": false,
    "rust-analyzer.cargo.extraArgs": [
        "--target",
        "x86_64-unknown-none"
    ],
    "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
    "workbench.editorAssociations": {
        "{hexdiff}:/**/*.*": "hexEditor.hexedit",
        "stage2": "hexEditor.hexedit",
        "stage1.bin": "hexEditor.hexedit",
        "loader": "hexEditor.hexedit"
    }
}